<!doctype html>
<html lang="en">
 <head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Hope For Homeless - Make a Difference</title>
  <script src="/_sdk/element_sdk.js"></script>
  <style>
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    html, body {
      height: 100%;
      box-sizing: border-box;
    }

    body {
      font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif;
      overflow-x: hidden;
    }

    .page {
      min-height: 100%;
      display: none;
    }

    .page.active {
      display: block;
    }

    /* Home Page Styles */
    .hero {
      background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
      color: white;
      padding: 80px 20px;
      text-align: center;
    }

    .hero h1 {
      font-size: 3em;
      margin-bottom: 20px;
      font-weight: 700;
    }

    .hero p {
      font-size: 1.3em;
      margin-bottom: 40px;
      opacity: 0.95;
    }

    .donate-btn {
      background: white;
      color: #667eea;
      padding: 18px 50px;
      font-size: 1.2em;
      border: none;
      border-radius: 50px;
      cursor: pointer;
      font-weight: 600;
      transition: all 0.3s ease;
      box-shadow: 0 10px 30px rgba(0,0,0,0.2);
    }

    .donate-btn:hover {
      transform: translateY(-3px);
      box-shadow: 0 15px 40px rgba(0,0,0,0.3);
    }

    .container {
      max-width: 1200px;
      margin: 0 auto;
      padding: 60px 20px;
    }

    .mission {
      text-align: center;
      margin-bottom: 60px;
    }

    .mission h2 {
      font-size: 2.5em;
      color: #2d3748;
      margin-bottom: 20px;
    }

    .mission p {
      font-size: 1.2em;
      color: #4a5568;
      line-height: 1.8;
      max-width: 800px;
      margin: 0 auto;
    }

    .gallery {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
      gap: 30px;
      margin-top: 50px;
    }

    .gallery-item {
      border-radius: 15px;
      overflow: hidden;
      box-shadow: 0 10px 30px rgba(0,0,0,0.15);
      transition: transform 0.3s ease;
    }

    .gallery-item:hover {
      transform: translateY(-10px);
    }

    .gallery-item svg {
      width: 100%;
      height: 250px;
    }

    .gallery-caption {
      padding: 20px;
      background: white;
      text-align: center;
    }

    .gallery-caption h3 {
      color: #2d3748;
      margin-bottom: 10px;
      font-size: 1.3em;
    }

    .gallery-caption p {
      color: #718096;
      line-height: 1.6;
    }

    .impact {
      background: #f7fafc;
      padding: 60px 20px;
      margin-top: 60px;
    }

    .stats {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
      gap: 30px;
      max-width: 1000px;
      margin: 0 auto;
    }

    .stat-card {
      background: white;
      padding: 30px;
      border-radius: 15px;
      text-align: center;
      box-shadow: 0 5px 20px rgba(0,0,0,0.1);
    }

    .stat-number {
      font-size: 3em;
      color: #667eea;
      font-weight: 700;
      margin-bottom: 10px;
    }

    .stat-label {
      color: #4a5568;
      font-size: 1.1em;
    }

    footer {
      background: #2d3748;
      color: white;
      text-align: center;
      padding: 40px 20px;
      margin-top: 60px;
    }

    /* Payment Page Styles */
    .payment-page {
      background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
      min-height: 100%;
      padding: 40px 20px;
    }

    .payment-container {
      max-width: 600px;
      margin: 0 auto;
      background: white;
      border-radius: 20px;
      padding: 40px;
      box-shadow: 0 20px 60px rgba(0,0,0,0.3);
    }

    .back-btn {
      background: #e2e8f0;
      color: #4a5568;
      border: none;
      padding: 12px 24px;
      border-radius: 8px;
      cursor: pointer;
      font-size: 1em;
      margin-bottom: 30px;
      transition: background 0.3s ease;
    }

    .back-btn:hover {
      background: #cbd5e0;
    }

    .payment-header {
      text-align: center;
      margin-bottom: 40px;
    }

    .payment-header h2 {
      color: #2d3748;
      font-size: 2.2em;
      margin-bottom: 10px;
    }

    .payment-header p {
      color: #718096;
      font-size: 1.1em;
    }

    .crypto-icon {
      width: 80px;
      height: 80px;
      margin: 0 auto 20px;
      display: block;
    }

    .wallet-section {
      background: #f7fafc;
      padding: 30px;
      border-radius: 15px;
      margin-bottom: 30px;
    }

    .wallet-label {
      color: #4a5568;
      font-size: 1em;
      margin-bottom: 10px;
      font-weight: 600;
    }

    .wallet-address {
      background: white;
      padding: 15px;
      border-radius: 8px;
      word-break: break-all;
      font-family: 'Courier New', monospace;
      color: #2d3748;
      border: 2px solid #e2e8f0;
      margin-bottom: 15px;
    }

    .copy-btn {
      background: #667eea;
      color: white;
      border: none;
      padding: 12px 30px;
      border-radius: 8px;
      cursor: pointer;
      font-size: 1em;
      width: 100%;
      transition: background 0.3s ease;
      font-weight: 600;
    }

    .copy-btn:hover {
      background: #5568d3;
    }

    .copy-btn.copied {
      background: #48bb78;
    }

    .qr-section {
      text-align: center;
      margin-bottom: 30px;
    }

    .qr-code {
      background: white;
      padding: 20px;
      border-radius: 15px;
      display: inline-block;
      box-shadow: 0 5px 20px rgba(0,0,0,0.1);
    }

    .instructions {
      background: #fff5e1;
      padding: 20px;
      border-radius: 10px;
      border-left: 4px solid #f59e0b;
    }

    .instructions h3 {
      color: #92400e;
      margin-bottom: 15px;
      font-size: 1.2em;
    }

    .instructions ol {
      color: #78350f;
      padding-left: 20px;
      line-height: 1.8;
    }

    .instructions li {
      margin-bottom: 8px;
    }

    @media (max-width: 768px) {
      .hero h1 {
        font-size: 2em;
      }

      .hero p {
        font-size: 1.1em;
      }

      .mission h2 {
        font-size: 1.8em;
      }

      .payment-container {
        padding: 30px 20px;
      }
    }
  </style>
  <style>@view-transition { navigation: auto; }</style>
  <script src="/_sdk/data_sdk.js" type="text/javascript"></script>
